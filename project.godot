config_version=5

[addons]

resources_spreadsheet_view/array_color_tint=100.0
resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false

[application]

config/name="Fishing Game"
config/tags=PackedStringArray("starterkit")
run/main_scene="res://scenes/island_final.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
boot_splash/bg_color=Color(0.92549, 0.92549, 0.960784, 1)
boot_splash/image="res://splash-screen.png"
config/icon="res://icon.png"

[audio]

video/shader_cache/strip_debug=false

[autoload]

Audio="*res://scripts/audio.gd"
CameraTransition="*res://objects/camera_transition.tscn"
DragDropController="*res://addons/DragDrop3D/DragDropController.gd"
SignalBus="*res://scripts/Global/SignalBus.gd"
FishingManager="*res://scripts/Global/FishingManager.gd"
Global="*res://scripts/Global/global.gd"
UiManager="*res://scripts/Global/UIManager.gd"
TimeManager="*res://scripts/Global/TimeManager.gd"
MailManager="*res://scripts/Global/MailManager.gd"
ProgressVariables="*res://scripts/Global/ProgressVariables.gd"
BasketManager="*res://scripts/Global/BasketManager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1280
window/size/window_height_override=700
window/stretch/mode="viewport"
window/ios/allow_high_refresh_rate=false

[editor]

movie_writer/mjpeg_quality=1.0
movie_writer/movie_file="D:/Godot/test.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/DragDrop3D/plugin.cfg", "res://addons/finite_state_machine/plugin.cfg", "res://addons/resources_spreadsheet_view/plugin.cfg")

[importer_defaults]

animation_library={
"fbx/embedded_image_handling": 3,
"fbx/importer": 1,
"gltf/embedded_image_handling": 3
}

